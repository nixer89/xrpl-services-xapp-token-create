{
  "name": "xrpl-services-xapp-token-create",
  "version": "2.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4202 --open",
    "build": "ng build --configuration production",
    "upload": "ng build --configuration production & aws s3 --profile myevride rm s3://tokencreate-xapp.xrpl.services/ --recursive & aws s3 --profile myevride cp ./dist/xrpl-services-xapp-token-create s3://tokencreate-xapp.xrpl.services/ --recursive --include '*' --acl public-read --cache-control public,max-age=604800,no-transform",
    "invalidate": "aws cloudfront --profile myevride create-invalidation --distribution-id EUNS5JGENGQ5E --paths /*",
    "upload-test": "ng build --configuration production & aws s3 --profile myevride rm s3://tokencreate-test-xapp.xrpl.services/ --recursive & aws s3 --profile myevride cp ./dist/xrpl-services-xapp-token-create s3://tokencreate-test-xapp.xrpl.services/ --recursive --include '*' --acl public-read --cache-control public,max-age=604800,no-transform",
    "invalidate-test": "aws cloudfront --profile myevride create-invalidation --distribution-id E39RS81X6BSK4L --paths /*",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.0",
    "@angular/cdk": "^13.1.0",
    "@angular/common": "^13.1.0",
    "@angular/compiler": "^13.1.0",
    "@angular/core": "^13.1.0",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "^13.1.0",
    "@angular/material": "^13.1.0",
    "@angular/material-moment-adapter": "^13.1.0",
    "@angular/platform-browser": "^13.1.0",
    "@angular/platform-browser-dynamic": "^13.1.0",
    "@angular/router": "^13.1.0",
    "buffer": "^6.0.3",
    "copy-to-clipboard": "^3.3.1",
    "five-bells-condition": "^5.0.1",
    "moment": "^2.29.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "xrpl-tagged-address-codec": "^0.2.1",
    "xumm-sdk": "^0.4.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.0",
    "@angular/cli": "^13.1.0",
    "@angular/compiler-cli": "^13.1.0",
    "@types/jasmine": "^3.10.2",
    "@types/node": "^14.17.34",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.5.3"
  }
}
